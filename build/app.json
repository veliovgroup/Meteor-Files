{
  "name": "Meteor Files",
  "version": "1.3.17",
  "description": "Demo application for ostrio:files package",
  "repository": "https://github.com/VeliovGroup/Meteor-Files",
  "website": "https://github.com/VeliovGroup/Meteor-Files/tree/master/demo",
  "logo": "https://d14xs1qewsqjcd.cloudfront.net/assets/og-image-logo.png",
  "scripts": {
    "start": "node build/main.js"
  },
  "dependencies": {
    "progress": "*",
    "http-proxy": "*",
    "sockjs": "*",
    "keypress": "*",
    "stream-buffers": "*",
    "request": "*",
    "useragent": "*",
    "mongodb": "*",
    "mime": "*",
    "connect": "*",
    "fibers": "1.0.8",
    "meteor-promise": "0.5.1",
    "underscore": "1.5.2",
    "source-map-support": "https://github.com/meteor/node-source-map-support/tarball/1912478769d76e5df4c365e147f25896aee6375e",
    "semver": "4.1.0"
  },
  "env": {
    "ROOT_URL": {
      "description": "Full URL of your app, with protocol (scheme), like http://example.com",
      "required": true
    },
    "MONGO_URL": {
      "description": "Full URL to mongodb, with protocol (scheme), like mongodb://user:pass@host:port/db. Create free one at: https://mlab.com",
      "required": true
    }
  },
  "keywords": [
    "meteor",
    "node",
    "node.js",
    "files",
    "file",
    "streaming",
    "upload",
    "cdn"
  ],
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    }
  ],
  "engines": {
    "node": "0.10.41",
    "npm": "1.4.29"
  }
}
